import pandas as pd
import csv
import numpy as np
import sklearn

data_set = pd.read_csv('https://drive.google.com/file/d/1nQ1F_Wtx1rvVIUL_rbDc0YpmH0yo7f7j/view?usp=share_link')

from sklearn.datasets import make_blobs
X, y = make_blobs(n_samples=1698, centers=2, n_features=2, random_state=2)
print(X.shape, y.shape)

from sklearn.linear_model import LogisticRegression
from sklearn.datasets import make_blobs
from sklearn.metrics import accuracy_score

X, y = make_blobs(n_samples=1698, centers=2, n_features=2, random_state=2)
model = LogisticRegression(solver='lbfgs')
model.fit(X, y)
yhat = model.predict(X)
acc = accuracy_score(y, yhat)
print(acc)

from sklearn.linear_model import LogisticRegression
from sklearn.datasets import make_blobs
X, y = make_blobs(n_samples=1698, centers=2, n_features=2, random_state=2)
model = LogisticRegression(solver='lbfgs')
model.fit(X, y)
new_input = [[-122.7406674, -130.5720846]]
new_output = model.predict(new_input)
print(new_input, new_output)

yhat = model.predict(X)
print(X[0], yhat[0])

for i in range(1000):
    print(X[i], yhat[i])
